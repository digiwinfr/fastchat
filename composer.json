{
  "name": "digiwin/fastchat",
  "description": "A basic chat",
  "type": "project",
  "autoload": {
    "psr-4": {
      "Digiwin\\Fastchat\\": "src/"
    }
  },
  "extra": {
    "installer-types": ["npm-asset"],
    "installer-paths": {
      "www/assets/lib/{$name}/": ["type:npm-asset"]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "bramus/router": "~1.4",
    "npm-asset/bootstrap": "^4.3",
    "oomphinc/composer-installers-extender": "^1.1",
    "npm-asset/blueimp-md5": "^2.10",
    "npm-asset/dayjs": "^1.8",
    "npm-asset/markdown-it": "^8.4",
    "npm-asset/markdown-it-emoji": "^1.4"
  }
}
